{
  "expo": {
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "umar-zakaria",
            "project": "correl"
          }
        }
      ]
    },
    "name": "Corral Rewards",
    "slug": "mCorrel",
    "version": "3.1.3",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mcorrel.app",
      "config": {
        "googleMapsApiKey": "AIzaSyBkD3xZnGnfJCFox8jYsmDYrhrreeOJ0wY"
      }
    },
    "android": {
      "versionCode": 27,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-splash.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mcorrel.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBUYLdMtAjONN7qO3zuIwDoM6z9C6TCf68"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo",
      ["expo-notifications"]
    ],
    "extra": {
      "eas": {
        "projectId": "210422cc-371e-4843-9197-26d3892c2c1a"
      }
    }
  }
}
